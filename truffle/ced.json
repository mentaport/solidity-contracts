{
  "projectId": "blockchain-contracts",
  "defaultEnvironment": "development",
  "subsystemId": "solidity-contracts",
  "build": {
    "steps": [
      {
        "name": "Install",
        "command": "npm install"
      },
      {
        "name": "Build",
        "command": "npm run compile"
      },
      {
        "name": "Prepare",
        "command": "mv publish_package.json package.json"
      }
    ],
    "artifact": "."
  }
}
